<odoo>

    <data noupdate="0">
        <record id="fme" model="res.partner">
            <field name="name">FME</field>
        </record>
        <record id="base.partner_root" model="res.partner">
            <field name="name">Administrator2</field>
        </record>
    </data>
    <data noupdate="1">
        <record id="fetch_mail_server_odoo" model="fetchmail.server">
            <field name="name">imap.openerp.com</field>
