<odoo>
	<record id="badminton" model="tournament">
		<field name="name">Badminton Singles template</field>
		<field name="match_type">sets</field>
		<field name="players_per_team">1</field>
	</record>

	<!-- Probably a terrible idea to use companies as clubs :p -->
    <record id="orneau_company" model="res.company">
        <field name="name">Orneau BC</field>
    </record>

	<record id="michael" model="res.partner">
		<field name="name">Mika</field>
        <field name="company_id" ref="orneau_company"/>
        <field name="customer" eval="False"/>
        <field name="is_player" eval="True"/>
        <field name="gender">male</field>
        <field name="email">mika@orneau.example.com</field>
        <field name="country_id" ref="base.be"/>
        <field name="tz">Europe/Brussels</field>
    </record>
	<record id="tournament_team_michael" model="tournament.team">
		<field name="player_ids" eval="[(4, ref('tournament.michael'), 0)]"/>
	</record>

